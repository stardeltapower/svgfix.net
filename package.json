{
  "name": "svgfix",
  "version": "1.0.0",
  "description": "Free online tool to normalize SVG viewBox and crop whitespace",
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "lint": "eslint . --ext .js,.ts,.astro,.vue",
    "lint:fix": "eslint . --ext .js,.ts,.astro,.vue --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "astro check"
  },
  "engines": {
    "node": ">=20.11.0",
    "pnpm": ">=8.15.0"
  },
  "dependencies": {
    "astro": "5.0.3",
    "vue": "3.5.13",
    "@astrojs/vue": "5.1.1",
    "@astrojs/tailwind": "5.1.3",
    "tailwindcss": "3.4.17",
    "svg-path-commander": "2.0.8",
    "svgo": "3.3.2"
  },
  "devDependencies": {
    "@astrojs/check": "0.9.4",
    "@playwright/test": "1.49.1",
    "@types/node": "22.10.5",
    "@typescript-eslint/eslint-plugin": "8.18.2",
    "@typescript-eslint/parser": "8.18.2",
    "@vitest/coverage-v8": "2.1.8",
    "eslint": "8.56.0",
    "eslint-plugin-astro": "1.3.1",
    "eslint-plugin-vue": "9.32.0",
    "prettier": "3.2.5",
    "prettier-plugin-astro": "0.14.1",
    "typescript": "5.7.2",
    "vite": "6.0.7",
    "vitest": "2.1.8"
  },
  "keywords": [
    "svg",
    "viewbox",
    "normalize",
    "crop",
    "whitespace",
    "transform",
    "tool",
    "online",
    "free"
  ],
  "author": {
    "name": "StarDelta",
    "url": "https://stardelta.io"
  },
  "license": "MIT",
  "homepage": "https://svgfix.net",
  "repository": {
    "type": "git",
    "url": "https://github.com/stardeltaio/svgfix.net.git"
  },
  "bugs": {
    "url": "https://github.com/stardeltaio/svgfix.net/issues"
  },
  "funding": {
    "type": "buymeacoffee",
    "url": "https://buymeacoffee.com/stardelta"
  }
}
